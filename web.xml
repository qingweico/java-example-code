<?xml version="1.0" encoding="UTF-8" ?>
<web-app>

	<!-- user -->
	<servlet>
		<servlet-name>user</servlet-name>
		<servlet-class>io.tomcat.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>user</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	<!-- login -->
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>io.tomcat.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>


</web-app>